{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/employees",
      "destination": "/api/employees"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "env": {
    "VERCEL_FORCE_NO_BUILD_CACHE": "1"
  }
}